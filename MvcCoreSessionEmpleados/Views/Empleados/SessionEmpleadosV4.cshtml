@{
}
@model List<Empleado>
<h1 style="color:blue"> Session empleados List OK</h1>

<p>
    <a asp-controller="Empleados" asp-action="EmpleadosAlmacenadosv4">Empleados almacenados v4</a>
</p>

<h4>@ViewData["MENSAJE"]</h4>

@if (Model != null)
{
    <ul class="list-group">
        @foreach (Empleado empleado in Model)
        {
            <li class="list-group-item">
                @empleado.Apellido
                <a asp-controller="Empleados" asp-action="SessionEmpleadosV4" asp-route-idEmpleado="@empleado.IdEmpleado">Almacenar empleado</a>
            </li>
        }
    </ul>
}